<Application x:Class="SnakeGame.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SnakeGame"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="BackgroundColor">#B61E7E</SolidColorBrush>
        <SolidColorBrush x:Key="GridBackgroundColor">#5F75F0</SolidColorBrush>
        <SolidColorBrush x:Key="GridLineColor">#4F48E7</SolidColorBrush>
        <SolidColorBrush x:Key="TextColor">#1EEBEB</SolidColorBrush>
        <SolidColorBrush x:Key="OverLayColor">#7F000000</SolidColorBrush>
        <FontFamily x:Key="MainFont">Assets/#Droid Sans Mono</FontFamily>
    </Application.Resources>
</Application>
